  <DataSets>
    <DataSet Name="dsTerritoryOrders">
      <Query>
        <DataSourceName>dscAdventureWorks</DataSourceName>
        <CommandText>select t.Name as TerritoryName
     , SUM(o.TotalDue) as TotalOrders
     , COUNT(*) as NumberOrders
from Sales.SalesOrderHeader o
inner join Sales.Customer c
     on o.CustomerID=c.CustomerID
inner join Sales.SalesTerritory t
     on c.TerritoryID=t.TerritoryID
group by t.Name</CommandText>
      </Query>
      <Fields>
        <Field Name="TerritoryName">
          <DataField>TerritoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalOrders">
          <DataField>TotalOrders</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NumberOrders">
          <DataField>NumberOrders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
